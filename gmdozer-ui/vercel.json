{
    "version": 2,
    "builds": [
    {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/index.html"
        }
    ],
    "env": {
        "NEXT_PUBLIC_BACKEND_URL": "https://gmdozer.de.r.appspot.com"
    }
}